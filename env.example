# Ollama 설정
NEXT_PUBLIC_OLLAMA_BASE_URL=http://localhost:11434
NEXT_PUBLIC_DEFAULT_AI_MODEL=llama3

# AWS 설정 (GitHub Secrets에 설정)
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=ap-northeast-2

# S3 버킷 설정
S3_BUCKET_PRODUCTION=nextjs-cicd-mcp-practice-bucket
S3_BUCKET_STAGING=nextjs-cicd-mcp-practice-staging-bucket

# CloudFront 설정
CLOUDFRONT_DISTRIBUTION_ID_PRODUCTION=your-production-distribution-id
CLOUDFRONT_DISTRIBUTION_ID_STAGING=your-staging-distribution-id

# Slack 알림 (선택사항)
SLACK_WEBHOOK_URL=your-slack-webhook-url

# 기타 설정
NODE_ENV=development